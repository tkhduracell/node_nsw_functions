{
  "name": "nackswinget",
  "description": "An Ionic app for Nackswinget",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "node": "20"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "ios:watch": "vite dev --host=0.0.0.0 --port=3000",
    "ios:open": "cap open ios",
    "ios:open:live": "cap run ios -l --external",
    "ios:sync": "vue-tsc && vite build && cap sync ios && cap open ios",
    "android:watch": "vite dev --host=0.0.0.0 --port=3000",
    "android:open": "cap open android",
    "android:open:live": "cap run android -l --external",
    "android:sync": "vue-tsc && vite build && cap sync android",
    "android:build": "cd android && ./gradlew --no-daemon bundleRelease",
    "android:upload": "cd android && ./gradlew --no-daemon publishBundle",
    "test:e2e": "cypress run",
    "test:unit": "vitest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@capacitor-community/fcm": "^5.0.2",
    "@capacitor/android": "5.6.0",
    "@capacitor/app": "5.0.7",
    "@capacitor/core": "5.6.0",
    "@capacitor/haptics": "5.0.7",
    "@capacitor/ios": "^5.6.0",
    "@capacitor/keyboard": "5.0.8",
    "@capacitor/push-notifications": "^5.1.1",
    "@capacitor/status-bar": "5.0.7",
    "@capacitor/toast": "^5.0.7",
    "@ionic/vue": "^7.7.0",
    "@ionic/vue-router": "^7.7.0",
    "@vueuse/core": "^10.7.2",
    "axios": "^1.7.2",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.1.3",
    "ionicons": "^7.2.2",
    "lodash": "^4.17.21",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@capacitor/cli": "5.6.0",
    "@types/lodash": "^4.14.202",
    "@vitejs/plugin-legacy": "^5.3.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.4",
    "cypress": "^14.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.21.1",
    "jsdom": "^24.0.0",
    "terser": "^5.39.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vitest": "^1.2.2",
    "vue-tsc": "^1.8.27"
  }
}