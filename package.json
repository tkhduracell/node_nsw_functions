{
  "name": "node_firebase_nackswinget",
  "version": "1.0.0",
  "description": "Utility HTTP functions related to nackswinget",
  "main": "dist/index.js",
  "engines": {
    "node": "16"
  },
  "scripts": {
    "pre-build": "node node_modules/puppeteer/install.js",
    "build": "tsc",
    "test": "tsc && node --loader ts-node/esm --inspect src/test.ts",
    "dev": "live-server --https=https.config.js --port=3000 --watch=static/ --mount=/:static/index.html --mount=/firebase-messaging-sw.js:static/firebase-messaging-sw.js",
    "dev-napi": "node --loader ts-node/esm --inspect src/notifications-api.ts",
    "clean": "rm -rf dist"
  },
  "author": "Filip Lindqvist",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/functions-framework": "^3.3.0",
    "@google-cloud/storage": "^6.12.0",
    "cheerio": "^1.0.0-rc.12",
    "cross-fetch": "^3.1.5",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "firebase-admin": "^11.5.0",
    "ical-generator": "^5.0.0",
    "lodash": "^4.17.21",
    "puppeteer": "^19.11.1",
    "source-map-support": "^0.5.21",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@tsconfig/node16": "^16.1.0",
    "@types/lodash": "^4.14.191",
    "concurrently": "^7.6.0",
    "dotenv-cli": "^7.0.0",
    "live-server": "^1.2.1",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
