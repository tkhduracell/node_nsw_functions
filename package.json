{
  "name": "node_firebase_nackswinget",
  "version": "1.0.0",
  "description": "Utility HTTP functions related to nackswinget",
  "main": "dist/index.js",
  "engines": {
    "node": "20"
  },
  "scripts": {
    "pre-build": "node node_modules/puppeteer/install.js",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "dev:calendars-api": "node --loader ts-node/esm --inspect src/calendars-api.ts",
    "dev:calendars-update-api": "node --loader ts-node/esm --inspect src/calendars-update-api.ts",
    "dev:notifications-api": "node --loader ts-node/esm --inspect src/notifications-api.ts",
    "dev:index": "live-server --https=https.config.js --port=3000 --watch=static/ --mount=/:static/index.html --mount=/firebase-messaging-sw.js:static/firebase-messaging-sw.js",
    "demo": "node --loader ts-node/esm --inspect src/demo.ts",
    "clean": "rm -rf dist"
  },
  "author": "Filip Lindqvist",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/functions-framework": "^3.3.0",
    "@google-cloud/storage": "^7.5.0",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "cross-fetch": "^4.0.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "dotenv": "^16.3.1",
    "firebase-admin": "^11.11.0",
    "ical-generator": "^6.0.0",
    "lodash": "^4.17.21",
    "puppeteer": "^21.4.1",
    "source-map-support": "^0.5.21",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.2",
    "@types/cors": "^2.8.15",
    "@types/jest": "^29.5.8",
    "@types/lodash": "^4.14.200",
    "concurrently": "^8.2.2",
    "dotenv-cli": "^7.3.0",
    "jest": "^29.7.0",
    "live-server": "^1.2.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
